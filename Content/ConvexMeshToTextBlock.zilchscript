[RunInEditor]
class ConvexMeshToTextBlock : ToTextBlockBase
{
  [Property] var Input : ConvexMesh;
  
  [Property]
  function Save()
  {
    if(this.Input == null)
      return;
    
    var fileName = this.GenerateVertexFileName(this.Input.Name);
    this.SaveVertices(fileName, this.Input.Vertices.ToArray());
  }
}
